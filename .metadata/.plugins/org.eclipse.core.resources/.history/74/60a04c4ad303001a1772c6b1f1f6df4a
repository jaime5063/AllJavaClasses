package com.class17;

public class ObjectsOfCarClass {
	
	public static void main(String[] args) {
		
		//to create an object syntax is
		//ClassName variable=new ClassName();
		
		CarExample car1=new CarExample();
		
		car1.make="Tesla";
		car1.model="S";
		car1.color="Black";
		car1.year=2020;
		car1.wheel=4;
		car1.windows=5;
		
		
		
		
		
		
		
		
		
	}

}
