package com.review7;

public class mathOperations {
	
	void add(int number1, int number2, int number3) {
		int sum = number1 + number2 + number3;
		System.out.println("The result of "+number1+" plus "+number2+" plus "+number3+" is = "+sum);
	}
	
	void subtract(int number1, int number2) {
		int sub = number1 - number2;
		System.out.println("The result of "+number1+" minus "+number2+" is = "+sub);
	}
	
	void muliply(double num1, double num2) {
		double times = num1 * num2;
		System.out.println("The result of "+num1+" times "+num2+" is = "+times);
	}

}
