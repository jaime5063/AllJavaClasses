package com.class9;

public class NestedLoopIntro {
	
	public static void main (String[] args) {
		
	// Nested Loops
		
	// our outer loop controls repetition of inner loop	
		
		for (int j=1; j<=3; j++) {
			System.out.println("I am outer loop");
			for(int i=1; i<=3; i++) {
				System.out.println("I am inner loop");
			}
		}
		
		
		
		
		
		
	}

}
