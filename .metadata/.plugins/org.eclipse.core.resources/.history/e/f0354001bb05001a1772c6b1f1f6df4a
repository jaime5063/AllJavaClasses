package com.class18;

public class Task {
	
	public static void main(String[] args) {
	
	Task obj=new Task();
	obj.largerNum(30, 10);

	}

	void largerNum(int a, int b) {
		
		if(a>b) {
			System.out.println(a+" is the larger number");
		}else {
			System.out.println(b+" is the larger number");
		}
		
	}
	
	
	
}